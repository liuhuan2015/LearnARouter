<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical"
    tools:context=".MainActivity">

    <Button
        android:id="@+id/btn_goto_simple_activity"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="简单跳转" />

    <Button
        android:id="@+id/btn_jump_with_argu"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="带参数跳转" />

    <Button
        android:id="@+id/btn_jump_by_uri"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="通过URI匹配进行跳转" />

    <Button
        android:id="@+id/btn_jump_with_interceptor"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="拦截器使用" />

    <Button
        android:id="@+id/btn_mock_startActivityForResult"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="ARouter中类似startActivityForResult的使用" />

</LinearLayout>